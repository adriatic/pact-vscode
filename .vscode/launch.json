{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run PACT (No Workspace)",
      "type": "extensionHost",
      "request": "launch",
      "runtimeExecutable": "${execPath}",
      "args": [
        "--extensionDevelopmentPath=${workspaceFolder}",
        "--new-window"
      ]
    }
  ]
}